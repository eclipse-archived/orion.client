{
  "name": "orion-client",
  "version": "1.0.0",
  "description": "The client side code for Eclipse Orion",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipse/orion.client.git"
  },
  "keywords": [
    "Orion"
  ],
  "babel": {
    "plugins": [
      "add-module-exports"
     ],
    "ignore": [
      "bundles/org.eclipse.orion.client.javascript/web/js-tests",
      "bundles/org.eclipse.orion.client.ui/web/mixloginstatic/javascript/jquery.js",
      "bundles/org.eclipse.orion.client.ui/web/plugins/languages/json/jslint.js",
      "bundles/org.eclipse.orion.client.core/web/chai/chai.js",
      "bundles/org.eclipse.orion.client.ui/web/gcli"
    ],
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ]
    ]
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/eclipse/orion.client/issues"
  },
  "homepage": "https://github.com/eclipse/orion.client#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "file-loader": "^0.9.0",
    "i18n-webpack-plugin": "^0.3.0",
    "image-webpack-loader": "^3.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "raw-loader": "^0.5.1",
    "webpack": "^2.2"
  },
  "dependencies": {}
}

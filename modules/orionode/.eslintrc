rules:
  block-scoped-var: 1
  camelcase: 0
  comma-dangle: 0
  comma-spacing: 1
  consistent-return: 1
  eol-last: 0
  eqeqeq: 2
  no-bitwise: 1
  no-caller: 2
  no-debugger: 2
  no-cond-assign: 2
  no-extra-semi: 1
  no-mixed-requires: 1
  no-mixed-spaces-and-tabs: [1, "smart-tabs"]
  no-multi-spaces: 0
  no-array-constructor: 1
  no-fallthrough: 2
  no-new: 1
  no-path-concat: 2
  no-proto: 2
  no-shadow-restricted-names: 2
  no-trailing-spaces: 0
  no-undef: 2
  no-undefined: 1
  no-underscore-dangle: 0
  no-unreachable: 1
  no-unused-expressions: 0
  no-unused-vars: 1
  no-use-before-define: [2, "nofunc"]
  no-sync: 2
  quotes: 0
  semi-spacing: 1

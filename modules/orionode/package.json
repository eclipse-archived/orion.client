{
  "name": "orion",
  "version": "20.0.0",
  "description": "An Eclipse Orion server written in Node.js.",
  "homepage": "http://eclipse.org/orion/",
  "bugs": "https://bugs.eclipse.org/bugs/enter_bug.cgi?product=Orion&component=Node",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "archiver": "^3.0.0",
    "async": "^0.9.0",
    "basic-auth": "^1.0.3",
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "compression": "^1.6.0",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "^1.4.1",
    "cron-parser": "^2.4.4",
    "csurf": "^1.9.0",
    "diff": "^3.1.0",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "fs-ext": "^1.0.0",
    "fs-extra": "^0.26.7",
    "generate-password": "^1.1.1",
    "google-protobuf": "^3.0.0-alpha.6.2",
    "graceful-cluster": "0.0.5",
    "graceful-fs": "^4.1.11",
    "https": "^1.0.0",
    "js-yaml": "^3.12.1",
    "jsonwebtoken": "^8.5.1",
    "lockfile": "^1.0.1",
    "log4js": "^3.0.6",
    "lru-cache-for-clusters-as-promised": "^1.5.15",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "mongodb": "^2.1.7",
    "mongoose": "^4.4.6",
    "multiparty": "^4.1.2",
    "nconf": "^0.9.1",
    "nodegit": "0.23.0",
    "nodemailer": "^6.0.0",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "passport-local-mongoose-email": "^0.1.2",
    "request": "2.87.0",
    "response-time": "^2.3.2",
    "rimraf": "~2.2.6",
    "semver": "^5.3.0",
    "socket.io": "~2.2.0",
    "tiny-worker": "^1.1.4",
    "xterm": "^2.3.2",
    "yaml-ast-parser": "0.0.28",
    "yauzl": "^2.8.0"
  },
  "experimentalDependencies": {
    "node-pty": "^0.6.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.0.0",
    "coveralls": "^3.0.3",
    "electron": "4.1.1",
    "eslint": "^5.12.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-requirejs": "^1.0.0",
    "grunt-istanbul": "^0.8.0",
    "grunt-karma": "^3.0.1",
    "grunt-mocha-istanbul": "^5.0.2",
    "grunt-simple-mocha": "^0.4.1",
    "grunt-string-replace": "^1.3.1",
    "jasmine": "^2.8.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "mocha": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^13.3.0",
    "requirejs": "^2.3.3",
    "superagent": "^3.5.1",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "^10.13.0"
  },
  "main": "server.js",
  "scripts": {
    "dist:osx": "build --o --publish=never",
    "dist:win": "build --w",
    "dist:linux": "build --l",
    "test": "grunt test_all",
    "report-coverage": "cat ./coverage/combined/lcov.info | coveralls",
    "optimize": "grunt",
    "watch-server": "nodemon --debug server.js"
  },
  "build": {
    "appId": "org.eclipse.orion",
    "asar": true,
    "electronVersion": "1.4.10",
    "productName": "orion",
    "nodeGypRebuild": "false",
    "npmRebuild": "false",
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns"
    },
    "dmg": {
      "title": "Orion",
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "build/icon",
      "target": [
        "squirrel"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "squirrelWindows": {
      "loadingGif": "build/orionLogo.gif",
      "iconUrl": "https://github.com/eclipse/orion.client/raw/master/modules/orionode/build/icon.ico",
      "remoteReleases": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/orion.client"
  },
  "keywords": [
    "orion",
    "eclipse",
    "ide"
  ],
  "author": "Orion contributors <orionnode@gmail.com>",
  "license": "EPL-1.0",
  "requirejsExcludeModules": [
    "GerritFilePlugin",
    "GitHubFilePlugin",
    "ServerStatus"
  ],
  "bin": {
    "orion": "./bin-global/orion.js"
  }
}
